{
  "name": "mdxld",
  "version": "0.1.0",
  "description": "Markdown & MDX Parser with YAML-LD Frontmatter Support",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./ast": {
      "types": "./dist/ast.d.ts",
      "import": "./dist/ast.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:contexts && tsc --project tsconfig.json",
    "build:contexts": "mkdir -p src/contexts/build && node scripts/build-contexts.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"*.{md,mdx}\"",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "markdown",
    "mdx",
    "yaml",
    "yaml-ld",
    "frontmatter",
    "linked-data",
    "ast"
  ],
  "author": "AI Primitives",
  "license": "MIT",
  "homepage": "https://mdx.org.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-primitives/mdxld.git"
  },
  "bugs": {
    "url": "https://github.com/ai-primitives/mdxld/issues"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "jsonld": "^8.3.2",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/json5": "^2.2.0",
    "@types/jsonld": "^1.5.15",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "axios": "^1.7.9",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.1.0",
    "remark-parse": "^11.0.0",
    "semantic-release": "^24.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
